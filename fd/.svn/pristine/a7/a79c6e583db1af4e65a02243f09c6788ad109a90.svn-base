@charset "utf-8";
/* CSS Document */
/*css预设*/

*{margin:0; padding:0;}/*将所有HTML元素的默认边距清0*/
html,body{font-size:14px; font-family:"Microsoft Yahei",'Arial'; color:#000; background:#FFF; height:100%;overflow: hidden;}/*对HTML元素中的字体、颜色、背景色进行初始设置*/
ul li,ol li{list-style:none;}/*将列表标签的默认样式清除*/
a{text-decoration:none;outline:none; blr:expression(this.onFocus=this.blur()); color: #333;}/*将超链接的下划线去掉以及在ie6中点击出现的虚线框去掉*/
img{border:none;}/*图片的默认边框去掉 */
table{border-collapse:collapse;border-spacing:0;}
caption,th,td{font-weight:normal;text-align:left;}
input,textarea,select,button{font-size:100%;font-family:inherit;margin:0;padding:0;}
label,button{cursor:pointer}
textarea{white-space:pre;resize:none;border:1px solid #b8b8b8;}
article,aside,figcaption,figure,footer,header,hgroup,nav,section,summary{ display: block;}
select,input[type="text"]:not([class=data-txt]),input[type="number"],textarea{min-width:110px;border:1px solid #b8b8b8;height: 34px; border-radius: 3px;}
/*清楚浮动*/
.clearfix:after{content:""; display:block; clear:both; height:0; line-height:0; visibility:hidden;}
.clearfix{zoom:1;}/*解决ie6的兼容性问题*/
.fl{float: left;}
.fr{float: right;}
a:link,a:visited,a:hover,a:active{text-decoration: none;}
.clearpadding{padding:0;}
table{white-space: nowrap;}
.row{margin:0;}
.row>div{padding:7px;}
.row article{position:relative;background: #fff; padding:10px; border-radius: 2px;}

.Dbg{padding:15px; background: #fff; border-radius: 2px;}
.table>tbody>tr>th,.table>tbody>tr>td{text-align:center;white-space: nowrap; vertical-align: middle; cursor: pointer;}
.table tr th{font-weight: bold; background: #e0e0e0;}
.table-icon{display: inline-block; width: 25px; height: 25px; background: url(../images/icon.png) no-repeat; background-position: 0 0;}
.see{background-position:4px -92px;}
.del{background-position:4px -32px;}
.edit{background-position:4px 0px;}
.down{background-position:4px -120px;}
.btn{font-size: 16px;}
.btn-box{margin:10px 0;text-align: center;}

.mybtn{height:32px; padding:0 10px;background:#0899d6; color:#fff;border:none; border-radius:3px;}
.mybtn:hover{background: #0381b6;}


/****修改*/
.fixed-table-pagination .pagination a {
    padding:6px 10px;
}
/****extent*/
.extent{
	position: relative;
	left:7px;
	top:58px;
}
.ol-popup {
        position: absolute;
        background-color: white;
        -webkit-filter: drop-shadow(0 1px 4px rgba(0, 0, 0, 0.2));
        filter: drop-shadow(0 1px 4px rgba(0, 0, 0, 0.2));
        padding: 15px;
        border-radius: 10px;
        border: 1px solid #cccccc;
        bottom: 12px;
        left: -50px;
        min-width: 280px;
    }

    .ol-popup:after,
    .ol-popup:before {
        top: 100%;
        border: solid transparent;
        content: " ";
        height: 0;
        width: 0;
        position: absolute;
        pointer-events: none;
    }

    .ol-popup:after {
        border-top-color: white;
        border-width: 10px;
        left: 48px;
        margin-left: -10px;
    }

    .ol-popup:before {
        border-top-color: #cccccc;
        border-width: 11px;
        left: 48px;
        margin-left: -11px;
    }

    .ol-popup-closer {
        text-decoration: none;
        position: absolute;
        top: 2px;
        right: 8px;
    }

    .ol-popup-closer:after {
        content: "✖";
    }

    #popup-content {
        font-size: 14px;
    }
#tuli {
    width: 200px;
    height: 130px;
    position: absolute;
    right: 10px;
    bottom: 10px;
    z-index: 100;
    background-color: white;
}