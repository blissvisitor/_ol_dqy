#loadingPage .loading-shade{cursor:wait;display:block;width:100%;height:100%;opacity:0;filter:alpha(opacity=00);background:#fff;position:absolute;top:0;left:0;z-index:100009}
#loadingPage .loading-content{cursor:default;position:absolute;z-index:100019;width:auto;border:1px solid #bbb;height:80px;line-height:80px;padding-left:80px;padding-right:20px;border-radius:10px;color:#666;font-size:20px;left:0;top:0;background:url(../img/loading.gif) 5px 50% no-repeat #fff;box-shadow:0 1px 15px rgba(0,0,0,.175)}
